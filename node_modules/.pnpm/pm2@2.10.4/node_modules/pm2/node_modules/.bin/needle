#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/needle/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/needle@2.9.1/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules/pm2/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
else 
  exec node  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
fi
