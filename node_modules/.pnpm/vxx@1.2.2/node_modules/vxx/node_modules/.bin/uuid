#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/uuid/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/uuid@3.4.0/node_modules/uuid/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/uuid@3.4.0/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/vxx@1.2.2/node_modules/vxx/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/vxx@1.2.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
else 
  exec node  "$basedir/../../../../../uuid@3.4.0/node_modules/uuid/bin/uuid" "$@"
fi
