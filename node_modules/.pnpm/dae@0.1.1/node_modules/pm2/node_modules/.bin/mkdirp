#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/mkdirp@0.5.1/node_modules/mkdirp/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/mkdirp@0.5.1/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules/pm2/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../../../../../mkdirp@0.5.1/node_modules/mkdirp/bin/cmd.js" "$@"
fi
