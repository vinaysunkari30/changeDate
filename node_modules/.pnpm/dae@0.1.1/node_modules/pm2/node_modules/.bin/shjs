#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/shelljs@0.7.8/node_modules/shelljs/bin/shjs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/shelljs@0.7.8/node_modules/shelljs/bin/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/shelljs@0.7.8/node_modules/shelljs/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/shelljs@0.7.8/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules:/home/nxtwave/nodejs/coding-practices/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules/pm2/node_modules:/home/nxtwave/nodejs/coding-practices/coding-practice-2a/node_modules/.pnpm/pm2@2.10.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../shelljs@0.7.8/node_modules/shelljs/bin/shjs" "$@"
else 
  exec node  "$basedir/../../../../../shelljs@0.7.8/node_modules/shelljs/bin/shjs" "$@"
fi
